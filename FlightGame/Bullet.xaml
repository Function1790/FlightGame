<UserControl x:Class="FlightGame.Bullet"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FlightGame"
             mc:Ignorable="d" 
             d:DesignHeight="15" d:DesignWidth="15">
    <Grid>
        <Rectangle x:Name="rect" Fill="#7F4DA6FF"/>
        <Rectangle x:Name="rect2" Visibility="Hidden">
            <Rectangle.Fill>
                <ImageBrush ImageSource="Missile.png"/>
            </Rectangle.Fill>
        </Rectangle>
    </Grid>
</UserControl>
